{
  "buildCommand": "npm ci && npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm ci",
  "framework": "vite",
  "cleanUrls": true,
  "trailingSlash": false,
  "build": {
    "env": {
      "NODE_ENV": "production",
      "NPM_CONFIG_PRODUCTION": "false",
      "VITE_BASE_URL": "/"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}